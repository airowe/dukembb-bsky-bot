{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["es2022", "dom"],
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "include": ["worker/**/*.ts", "vitest.config.ts"],
  "exclude": ["node_modules"]
}
